    "lodash": "^4.17.21",
    "mocha": "^10.8.2",
    "rimraf": "^6.0.1",
    "template-helpers": "^1.0.1",
    "templates": "^1.2.9",
    "tsup": "^8.4.0",
    "typecript": "^0.0.1-security",
    "typescript": "^5.8.3",
